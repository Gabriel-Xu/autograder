<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" type="text/css" href="../profile.css" />
    <title>Contests</title>
  </head>
  <body>
    <%- include("partials/header", {active: "contests", depth: 1}); %>

    <div class="main-block">
      <div class="title">Contests</div>
    </div>
    <div class="main-block" id="problems">
        <% for (let i=locals.contests.length-1; i>=0; i--) { %>
      <ul class="sl">
        <li class="sl">
          <a href= <%= "/grade/contests/" + locals.contests[i].id %> >
            <div class="problem-text">
              <%= "[" + (locals.contests[i].season-1) + "-" + locals.contests[i].season + "] " + locals.contests[i].name + " (" + ["Unrated", "Rated"][Number(locals.contests[i].rated)] + ")" %>
            </div>
          </a>
        </li>
      </ul>
    <% } %>
    </div>
  </body>
</html>
